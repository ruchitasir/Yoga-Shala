<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   
    <title>Yoga Shala</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
   
     <link rel="stylesheet" href="/css/style.css">
   
   
</head>
<body>
   
    <%-include('partials/nav') %> 
    <%-include('partials/alerts') %> 
    <main> <%-body %> </main>
    <%-include('partials/footer') %> 
  
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js">
    </script>
   <script>
    document.addEventListener('DOMContentLoaded', function() {
        var elems = document.querySelectorAll('.sidenav');
        var instances = M.Sidenav.init(elems, {});

        var startDate = new Date()
        startDate.setFullYear(startDate.getFullYear() - 30)
        var elems2 = document.querySelectorAll('.datepicker')
        var instances2 = M.Datepicker.init(elems2, {
            autoClose: true,
            defaultDate: startDate,
            setDefaultDate: true,
            yearRange: 50,
            maxDate: (new Date())
        })
        var currentDate = new Date()
        var elems4 = document.querySelectorAll('.dateCurrent')
        var instances4 = M.Datepicker.init(elems4, {
            autoClose: true,
            defaultDate: currentDate,
            setDefaultDate: true,
            yearRange: 50
        })

        var elems3 = document.querySelectorAll('select');
        var instances3 = M.FormSelect.init(elems3, {}); 
    });
    
    $( document ).ready(function() {
        $(".dropdown-trigger").dropdown();
    });

</script>

</body>
</html>